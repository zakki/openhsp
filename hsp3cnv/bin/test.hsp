#cmpopt varname 1

	gosub *subr

	a=hspver
	mes "START"+strf("%x",a)
	mes "SIN="+sin(0.5+sqrt(1.0))

	a=0:b=0:c=0

	repeat 10
	a++
	b=rnd(5)+1
	mes "A="+a+"/B="+b
	c+=a/b
	loop

	mes "C="+c
	mes "END"
	stop

*subr
	repeat 10
	if cnt=3 : continue
	mes "OK"+cnt
	if cnt=5 : break
	loop
	return
