
	;
	;	objsel命令を使用したサンプル
	;	( [TAB]キーで入力フォーカスを移動する )
	;				for HSP ver2.4d
	;
	a1=""
	a2=""
	a3=""
	objsize 80,24,24
	pos 120,12
	mes "INPUT-1"
	mes "INPUT-2"
	mes "INPUT-3"
	pos 20,8
	input a1
	input a2
	input a3
	button "END",*owari
	;
	curid=0				; 現在のフォーカスID
	;
*keychk
	objsel -1:curid=stat
	title "ID="+curid
	wait 10
	goto *keychk

*owari
	end

